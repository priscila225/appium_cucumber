{
  "name": "cucumber-js-appium-app-browserstack",
  "version": "1.0.0",
  "readme": "Cucumber-JS Appium Integration with [BrowserStack](https://www.browserstack.com)",
  "description": "Cucumber-JS Appium Integration with BrowserStack",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserstack/cucumber-js-appium-app-browserstack.git"
  },
  "scripts": {
    "sample-test": "browserstack-node-sdk cucumber-js features/single.feature --format json:report/json-output/report.json --format progress",
    "sample-local-test": "browserstack-node-sdk cucumber-js features/local.feature",
    "postinstall": "npm update browserstack-node-sdk"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "assert": "^2.0.0",
    "browserstack-node-sdk": "latest"
  },
  "dependencies": {
    "browserstack-local": "^1.0.0",
    "cross-env": "^5.2.0",
    "selenium-webdriver": "4.1.2",
    "multiple-cucumber-html-reporter": "^3.9.3"
  }
}
